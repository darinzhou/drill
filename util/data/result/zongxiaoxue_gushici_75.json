[
    {
        "author": "邵雍", 
        "content": "一去二三里，\n烟村四五家。\n亭台六七座，\n八九十枝花。", 
        "explanation": "", 
        "note": "", 
        "prologue": "", 
        "style": "", 
        "subtitle": "", 
        "title": "山村"
    }, 
    {
        "author": "骆宾王", 
        "content": "鹅，\n鹅，\n鹅，\n曲项向天歌。\n白毛浮绿水，\n红掌拨清波。", 
        "explanation": "", 
        "note": "", 
        "prologue": "", 
        "style": "", 
        "subtitle": "", 
        "title": "咏鹅"
    }, 
    {
        "author": "王之涣", 
        "content": "白日依山尽，\n黄河入海流。\n欲穷千里目，\n更上一层楼。", 
        "explanation": "", 
        "note": "", 
        "prologue": "", 
        "style": "", 
        "subtitle": "", 
        "title": "登鹳雀楼"
    }, 
    {
        "author": "李白", 
        "content": "小时不识月，\n呼作白玉盘。\n又疑瑶台镜，\n飞在青云端。", 
        "explanation": "", 
        "note": "", 
        "prologue": "", 
        "style": "", 
        "subtitle": "", 
        "title": "古朗月行"
    }, 
    {
        "author": "李绅", 
        "content": "锄禾日当午，\n汗滴禾下土。\n谁知盘中餐，\n粒粒皆辛苦。", 
        "explanation": "", 
        "note": "", 
        "prologue": "", 
        "style": "", 
        "subtitle": "", 
        "title": "锄禾"
    }, 
    {
        "author": "王安石", 
        "content": "墙角数枝梅，\n凌寒独自开。\n遥知不是雪，\n为有暗香来。", 
        "explanation": "", 
        "note": "", 
        "prologue": "", 
        "style": "", 
        "subtitle": "", 
        "title": "梅花"
    }, 
    {
        "author": "李白", 
        "content": "床前明月光，\n疑是地上霜。\n举头望明月，\n低头思故乡。", 
        "explanation": "", 
        "note": "", 
        "prologue": "", 
        "style": "", 
        "subtitle": "", 
        "title": "静夜思"
    }, 
    {
        "author": "孟浩然", 
        "content": "春眠不觉晓，\n处处闻啼鸟。\n夜来风雨声，\n花落知多少。", 
        "explanation": "", 
        "note": "", 
        "prologue": "", 
        "style": "", 
        "subtitle": "", 
        "title": "春晓"
    }, 
    {
        "author": "杜甫", 
        "content": "两个黄鹂鸣翠柳，\n一行白鹭上青天。\n窗含西岭千秋雪，\n门泊东吴万里船。", 
        "explanation": "", 
        "note": "", 
        "prologue": "", 
        "style": "", 
        "subtitle": "", 
        "title": "绝句"
    }, 
    {
        "author": "白居易", 
        "content": "谁道群生性命微，\n一般骨肉一般皮。\n劝君莫打枝头鸟，\n子在巢中望母归。", 
        "explanation": "", 
        "note": "", 
        "prologue": "", 
        "style": "", 
        "subtitle": "", 
        "title": "鸟"
    }, 
    {
        "author": "杜甫", 
        "content": "好雨知时节，\n当春乃发生。\n随风潜入夜，\n润物细无声。", 
        "explanation": "", 
        "note": "", 
        "prologue": "", 
        "style": "", 
        "subtitle": "", 
        "title": "春夜喜雨"
    }, 
    {
        "author": "李白", 
        "content": "李白乘舟将欲行，\n忽闻岸上踏歌声。\n桃花潭水深千尺，\n不及汪伦送我情。", 
        "explanation": "", 
        "note": "", 
        "prologue": "", 
        "style": "", 
        "subtitle": "", 
        "title": "赠汪伦"
    }, 
    {
        "author": "汉乐府", 
        "content": "青青园中葵,\n朝露待日晞。\n阳春布德泽,\n万物生光辉。\n常恐秋节至,\n焜黄华叶衰。\n百川东到海,\n何时复西归。\n少壮不努力,\n老大徒伤悲。", 
        "explanation": "", 
        "note": "", 
        "prologue": "", 
        "style": "", 
        "subtitle": "", 
        "title": "长歌行"
    }, 
    {
        "author": "杜牧", 
        "content": "远上寒山石径斜，\n白云深处有人家。\n停车坐爱枫林晚，\n霜叶红于二月花。", 
        "explanation": "", 
        "note": "", 
        "prologue": "", 
        "style": "", 
        "subtitle": "", 
        "title": "山行"
    }, 
    {
        "author": "李白", 
        "content": "危楼高百尺，\n手可摘星辰。\n不敢高声语，\n恐惊天上人。", 
        "explanation": "", 
        "note": "", 
        "prologue": "", 
        "style": "", 
        "subtitle": "", 
        "title": "夜宿山寺"
    }, 
    {
        "author": "李白", 
        "content": "日照香炉生紫烟，\n遥看瀑布挂前川。\n飞流直下三千尺，\n疑是银河落九天。", 
        "explanation": "", 
        "note": "", 
        "prologue": "", 
        "style": "", 
        "subtitle": "", 
        "title": "望庐山瀑布"
    }, 
    {
        "author": "李峤", 
        "content": "解落三秋叶，\n能开二月花。\n过江千尺浪，\n入竹万竿斜。", 
        "explanation": "", 
        "note": "", 
        "prologue": "", 
        "style": "", 
        "subtitle": "", 
        "title": "风"
    }, 
    {
        "author": "白居易", 
        "content": "江南好，\n风景旧曾谙。\n日出江花红胜火，\n春来江水绿如蓝。\n能不忆江南？", 
        "explanation": "", 
        "note": "", 
        "prologue": "", 
        "style": "", 
        "subtitle": "", 
        "title": "忆江南"
    }, 
    {
        "author": "袁枚", 
        "content": "牧童骑黄牛，\n歌声振林樾。\n意欲捕鸣蝉，\n忽然闭口立。", 
        "explanation": "", 
        "note": "", 
        "prologue": "", 
        "style": "", 
        "subtitle": "", 
        "title": "所见"
    }, 
    {
        "author": "贺知章", 
        "content": "少小离家老大回，\n乡音无改鬓毛衰。\n儿童相见不相识，\n笑问客从何处来。", 
        "explanation": "", 
        "note": "", 
        "prologue": "", 
        "style": "", 
        "subtitle": "", 
        "title": "回乡偶书"
    }, 
    {
        "author": "贺知章", 
        "content": "碧玉妆成一树高，\n万条垂下绿丝绦。\n不知细叶谁裁出，\n二月春风似剪刀。", 
        "explanation": "", 
        "note": "", 
        "prologue": "", 
        "style": "", 
        "subtitle": "", 
        "title": "咏柳"
    }, 
    {
        "author": "寇准", 
        "content": "只有天在上，\n更无山与齐。\n举头红日近，\n回首白云低。", 
        "explanation": "", 
        "note": "", 
        "prologue": "", 
        "style": "", 
        "subtitle": "", 
        "title": "华山"
    }, 
    {
        "author": "李白", 
        "content": "朝辞白帝彩云间，\n千里江陵一日还。\n两岸猿声啼不住，\n轻舟已过万重山。", 
        "explanation": "", 
        "note": "", 
        "prologue": "", 
        "style": "", 
        "subtitle": "", 
        "title": "早发白帝城"
    }, 
    {
        "author": "李白", 
        "content": "故人西辞黄鹤楼，\n烟花三月下扬州。\n孤帆远影碧空尽，\n唯见长江天际流。", 
        "explanation": "", 
        "note": "", 
        "prologue": "", 
        "style": "", 
        "subtitle": "", 
        "title": "黄鹤楼送孟浩然之广陵"
    }, 
    {
        "author": "杨万里", 
        "content": "篱落疏疏一径深，\n树头花落未成阴。\n儿童急走追黄蝶，\n飞入菜花无处寻。", 
        "explanation": "", 
        "note": "", 
        "prologue": "", 
        "style": "", 
        "subtitle": "", 
        "title": "宿新市徐公店"
    }, 
    {
        "author": "卢纶", 
        "content": "林暗草惊风，\n将军夜引弓。\n平明寻白羽，\n没在石棱中。", 
        "explanation": "", 
        "note": "", 
        "prologue": "", 
        "style": "", 
        "subtitle": "", 
        "title": "塞下曲"
    }, 
    {
        "author": "杨万里", 
        "content": "泉眼无声惜细流，\n树阴照水爱晴柔。\n小荷才露尖尖角，\n早有蜻蜓立上头。", 
        "explanation": "", 
        "note": "", 
        "prologue": "", 
        "style": "", 
        "subtitle": "", 
        "title": "小池"
    }, 
    {
        "author": "张志和", 
        "content": "西塞山前白鹭飞，\n桃花流水鳜鱼肥。\n青箬笠，\n绿蓑衣，\n斜风细雨不须归。", 
        "explanation": "", 
        "note": "", 
        "prologue": "", 
        "style": "", 
        "subtitle": "", 
        "title": "渔歌子"
    }, 
    {
        "author": "杨万里", 
        "content": "毕竟西湖六月中，\n风光不与四时同。\n接天莲叶无穷碧，\n映日荷花别样红。", 
        "explanation": "", 
        "note": "", 
        "prologue": "", 
        "style": "", 
        "subtitle": "", 
        "title": "晓出净慈寺送林子方"
    }, 
    {
        "author": "杜甫", 
        "content": "锦城丝管日纷纷，\n半入江风半入云。\n此曲只应天上有，\n人间能得几回闻。", 
        "explanation": "", 
        "note": "", 
        "prologue": "", 
        "style": "", 
        "subtitle": "", 
        "title": "赠花卿"
    }, 
    {
        "author": "王维", 
        "content": "独坐幽篁里，\n弹琴复长啸。\n深林人不知，\n明月来相照。", 
        "explanation": "", 
        "note": "", 
        "prologue": "", 
        "style": "", 
        "subtitle": "", 
        "title": "竹里馆"
    }, 
    {
        "author": "陆游", 
        "content": "死去元知万事空，\n但悲不见九州同。\n王师北定中原日，\n家祭无忘告乃翁。", 
        "explanation": "", 
        "note": "", 
        "prologue": "", 
        "style": "", 
        "subtitle": "", 
        "title": "示儿"
    }, 
    {
        "author": "北朝民歌", 
        "content": "敕勒川，\n阴山下。\n天似穹庐，\n笼盖四野。\n天苍苍，\n野茫茫，\n风吹草低见牛羊。", 
        "explanation": "", 
        "note": "", 
        "prologue": "", 
        "style": "", 
        "subtitle": "", 
        "title": "敕勒歌"
    }, 
    {
        "author": "柳宗元", 
        "content": "千山鸟飞绝，\n万径人踪灭。\n孤舟蓑笠翁，\n独钓寒江雪。", 
        "explanation": "", 
        "note": "", 
        "prologue": "", 
        "style": "", 
        "subtitle": "", 
        "title": "江雪"
    }, 
    {
        "author": "杜牧", 
        "content": "银烛秋光冷画屏，\n轻罗小扇扑流萤。\n天阶夜色凉如水，\n坐看牵牛织女星。", 
        "explanation": "", 
        "note": "", 
        "prologue": "", 
        "style": "", 
        "subtitle": "", 
        "title": "秋夕"
    }, 
    {
        "author": "王安石", 
        "content": "爆竹声中一岁除，\n春风送暖入屠苏。\n千门万户曈曈日，\n总把新桃换旧符。", 
        "explanation": "", 
        "note": "", 
        "prologue": "", 
        "style": "", 
        "subtitle": "", 
        "title": "元旦"
    }, 
    {
        "author": "孟郊", 
        "content": "慈母手中线，\n游子身上衣。\n临行密密缝，\n意恐迟迟归。\n谁言寸草心，\n报得三春晖。", 
        "explanation": "", 
        "note": "", 
        "prologue": "", 
        "style": "", 
        "subtitle": "", 
        "title": "游子吟"
    }, 
    {
        "author": "高鼎", 
        "content": "草长莺飞二月天，\n拂堤杨柳醉春烟。\n儿童散学归来早，\n忙趁东风放纸鸢。", 
        "explanation": "", 
        "note": "", 
        "prologue": "", 
        "style": "", 
        "subtitle": "", 
        "title": "村居"
    }, 
    {
        "author": "朱熹", 
        "content": "半亩方塘一鉴开，\n天光云影共徘徊。\n问渠哪得清如许？\n为有源头活水来。", 
        "explanation": "", 
        "note": "", 
        "prologue": "", 
        "style": "", 
        "subtitle": "", 
        "title": "观书有感"
    }, 
    {
        "author": "于谦", 
        "content": "千锤万凿出深山，\n烈火焚烧若等闲。\n粉身碎骨浑不怕，\n要留清白在人间。", 
        "explanation": "", 
        "note": "", 
        "prologue": "", 
        "style": "", 
        "subtitle": "", 
        "title": "石灰吟"
    }, 
    {
        "author": "汉乐府", 
        "content": "江南可采莲，\n莲叶何田田，\n鱼戏莲叶间。\n鱼戏莲叶东，\n鱼戏莲叶西，\n鱼戏莲叶南，\n鱼戏莲叶北。", 
        "explanation": "", 
        "note": "", 
        "prologue": "", 
        "style": "", 
        "subtitle": "", 
        "title": "江南"
    }, 
    {
        "author": "范仲淹", 
        "content": "江上往来人，\n但爱鲈鱼美。\n君看一叶舟，\n出没风波里。", 
        "explanation": "", 
        "note": "", 
        "prologue": "", 
        "style": "", 
        "subtitle": "", 
        "title": "江上渔者"
    }, 
    {
        "author": "杜甫", 
        "content": "国破山河在，\n城春草木深。\n感时花溅泪，\n恨别鸟惊心。\n烽火连三月，\n家书抵万金。\n白头搔更短，\n浑欲不胜簪。", 
        "explanation": "", 
        "note": "", 
        "prologue": "", 
        "style": "", 
        "subtitle": "", 
        "title": "春望"
    }, 
    {
        "author": "王维", 
        "content": "人闲桂花落，\n夜静春山空。\n月出惊山鸟，\n时鸣春涧中。", 
        "explanation": "", 
        "note": "", 
        "prologue": "", 
        "style": "", 
        "subtitle": "", 
        "title": "鸟鸣涧"
    }, 
    {
        "author": "白居易", 
        "content": "一道残阳铺水中，\n半江瑟瑟半江红。\n可怜九月初三夜，\n露似珍珠月似弓。", 
        "explanation": "", 
        "note": "", 
        "prologue": "", 
        "style": "", 
        "subtitle": "", 
        "title": "暮江吟"
    }, 
    {
        "author": "辛弃疾", 
        "content": "明月别枝惊鹊，\n清风半夜鸣蝉。\n稻花香里说丰年，\n听取蛙声一片。\n七八个星天外，\n两三点雨山前。\n旧时茅店社林边，\n路转溪桥忽见。", 
        "explanation": "", 
        "note": "", 
        "prologue": "", 
        "style": "", 
        "subtitle": "", 
        "title": "西江月"
    }, 
    {
        "author": "杜牧", 
        "content": "青山隐隐水迢迢，\n秋尽江南草未凋。\n二十四桥明月夜，\n玉人何处教吹萧。", 
        "explanation": "", 
        "note": "", 
        "prologue": "", 
        "style": "", 
        "subtitle": "", 
        "title": "寄扬州韩绰判官"
    }, 
    {
        "author": "戴叔伦", 
        "content": "凉月如眉挂柳湾，\n越中山色镜中看。\n兰溪三日桃花雨，\n半夜鲤鱼来上滩。", 
        "explanation": "", 
        "note": "", 
        "prologue": "", 
        "style": "", 
        "subtitle": "", 
        "title": "兰溪棹歌"
    }, 
    {
        "author": "张旭", 
        "content": "隐隐飞桥隔野烟，\n石矶西畔问渔船。\n桃花尽日随流水，\n洞在清溪何处边。", 
        "explanation": "", 
        "note": "", 
        "prologue": "", 
        "style": "", 
        "subtitle": "", 
        "title": "桃花溪"
    }, 
    {
        "author": "杜甫", 
        "content": "黄四娘家花满蹊，\n千朵万朵压枝低。\n留连戏蝶时时舞，\n自在娇莺恰恰啼。", 
        "explanation": "", 
        "note": "", 
        "prologue": "", 
        "style": "", 
        "subtitle": "", 
        "title": "江畔独步寻花"
    }, 
    {
        "author": "叶绍翁", 
        "content": "应怜屐齿印苍苔，\n小扣柴扉久不开。\n春色满园关不住，\n一枝红杏出墙来。", 
        "explanation": "", 
        "note": "", 
        "prologue": "", 
        "style": "", 
        "subtitle": "", 
        "title": "游园不值"
    }, 
    {
        "author": "阮元", 
        "content": "交流四水抱城斜，\n散作千溪遍万家。\n深处种菱浅种稻，\n不深不浅种荷花。", 
        "explanation": "", 
        "note": "", 
        "prologue": "", 
        "style": "", 
        "subtitle": "", 
        "title": "吴兴杂诗"
    }, 
    {
        "author": "韦应物", 
        "content": "独怜幽草涧边生，\n上有黄鹂深树鸣。\n春潮带雨晚来急，\n野渡无人舟自横。", 
        "explanation": "", 
        "note": "", 
        "prologue": "", 
        "style": "", 
        "subtitle": "", 
        "title": "滁州西涧"
    }, 
    {
        "author": "查慎行", 
        "content": "月黑见渔灯，\n孤光一点萤。\n微微风簇浪，\n散作满河星。", 
        "explanation": "", 
        "note": "", 
        "prologue": "", 
        "style": "", 
        "subtitle": "", 
        "title": "舟夜书所见"
    }, 
    {
        "author": "杜甫", 
        "content": "胡马大宛名，\n锋棱瘦骨成。\n竹批双耳峻，\n风入四蹄轻。\n所向无空阔，\n真堪托死生。\n骁腾有如此，\n万里可横行。", 
        "explanation": "", 
        "note": "", 
        "prologue": "", 
        "style": "", 
        "subtitle": "", 
        "title": "房兵曹胡马"
    }, 
    {
        "author": "李贺", 
        "content": "大漠沙如雪，\n燕山月似钩。\n何当金络脑，\n快走踏清秋。", 
        "explanation": "", 
        "note": "", 
        "prologue": "", 
        "style": "", 
        "subtitle": "", 
        "title": "马诗"
    }, 
    {
        "author": "白居易", 
        "content": "白浪茫茫与海连，\n平沙浩浩四无边。\n暮去朝来淘不住，\n遂令东海变桑田。", 
        "explanation": "", 
        "note": "", 
        "prologue": "", 
        "style": "", 
        "subtitle": "", 
        "title": "浪淘沙"
    }, 
    {
        "author": "苏轼", 
        "content": "水光潋滟晴方好，\n山色空濛雨亦奇。\n欲把西湖比西子，\n淡妆浓抹总相宜。", 
        "explanation": "", 
        "note": "", 
        "prologue": "", 
        "style": "", 
        "subtitle": "", 
        "title": "饮湖上初晴后雨"
    }, 
    {
        "author": "李白", 
        "content": "炉火照天地，\n红星乱紫烟。\n赧郎明月夜，\n歌曲动寒川。", 
        "explanation": "", 
        "note": "", 
        "prologue": "", 
        "style": "", 
        "subtitle": "", 
        "title": "秋浦歌"
    }, 
    {
        "author": "罗隐", 
        "content": "不论平地与山尖，\n无限风光尽被占。\n采得百花成蜜后，\n为谁辛苦为谁甜。", 
        "explanation": "", 
        "note": "", 
        "prologue": "", 
        "style": "", 
        "subtitle": "", 
        "title": "蜂"
    }, 
    {
        "author": "王安石", 
        "content": "飞来山上千寻塔，\n闻说鸡鸣见日升。\n不畏浮云遮望眼，\n自缘身在最高层。", 
        "explanation": "", 
        "note": "", 
        "prologue": "", 
        "style": "", 
        "subtitle": "", 
        "title": "登飞来峰"
    }, 
    {
        "author": "苏轼", 
        "content": "横看成岭侧成峰，\n远近高低各不同。\n不识庐山真面目，\n只缘自在此山中。", 
        "explanation": "", 
        "note": "", 
        "prologue": "", 
        "style": "", 
        "subtitle": "", 
        "title": "题西林壁"
    }, 
    {
        "author": "李商隐", 
        "content": "君问归期未有期，\n巴山夜雨涨秋池。\n何当共剪西窗烛，\n却话巴山夜雨时。", 
        "explanation": "", 
        "note": "", 
        "prologue": "", 
        "style": "", 
        "subtitle": "", 
        "title": "夜雨寄北"
    }, 
    {
        "author": "张继", 
        "content": "月落乌啼霜满天，\n江枫渔火对愁眠。\n姑苏城外寒山寺，\n夜半钟声到客船。", 
        "explanation": "", 
        "note": "", 
        "prologue": "", 
        "style": "", 
        "subtitle": "", 
        "title": "枫桥夜泊"
    }, 
    {
        "author": "王之涣", 
        "content": "黄河远上白云间，\n一片孤城万仞山。\n羌笛何须怨杨柳，\n春风不度玉门关。", 
        "explanation": "", 
        "note": "", 
        "prologue": "", 
        "style": "", 
        "subtitle": "", 
        "title": "凉州词"
    }, 
    {
        "author": "刘禹锡", 
        "content": "九曲黄河万里沙，\n浪淘风簸自天涯。\n如今直上银河去，\n同到牵牛织女家。", 
        "explanation": "", 
        "note": "", 
        "prologue": "", 
        "style": "", 
        "subtitle": "", 
        "title": "浪淘沙"
    }, 
    {
        "author": "《乐府诗集》", 
        "content": "十五从军征，\n八十始得归。\n道逢乡里人：\n“家中有阿谁？\n”遥看是君家，\n松柏冢垒垒。\n兔从狗窦入，\n雉从梁上飞。\n中庭生杂树，\n井上生旅葵。\n舂谷持做饭，\n采葵持做羹。\n羹饭一时熟，\n不知贻阿谁。\n出门东向看，\n泪落沾我衣。", 
        "explanation": "", 
        "note": "", 
        "prologue": "", 
        "style": "", 
        "subtitle": "", 
        "title": "十五从军征"
    }, 
    {
        "author": "王昌龄", 
        "content": "秦时明月汉时关，\n万里长征人未还。\n但使龙城飞将在，\n不教胡马度阴山。", 
        "explanation": "", 
        "note": "", 
        "prologue": "", 
        "style": "", 
        "subtitle": "", 
        "title": "出塞"
    }, 
    {
        "author": "郑板桥", 
        "content": "衙斋卧听萧萧竹，\n疑是民间疾苦声。\n些小吾曹州县吏，\n一枝一叶总关情。", 
        "explanation": "", 
        "note": "", 
        "prologue": "", 
        "style": "", 
        "subtitle": "", 
        "title": "墨竹图题诗"
    }, 
    {
        "author": "李方膺", 
        "content": "君不见，\n岁之寒，\n何处求芳草。\n又不见，\n松之乔，\n青青复矫矫。\n天地本无心，\n万物贵其真。\n直干壮山岳，\n秀色无等伦。\n饱历冰与霜，\n千年方未已。\n拥护天阙高且坚，\n迥干春风碧云里。", 
        "explanation": "", 
        "note": "", 
        "prologue": "", 
        "style": "", 
        "subtitle": "", 
        "title": "苍松怪石图题诗"
    }, 
    {
        "author": "王冕", 
        "content": "吾家冼砚池头树,\n朵朵花开淡墨痕。\n不要人夸好颜色，\n只留清气满乾坤。", 
        "explanation": "", 
        "note": "", 
        "prologue": "", 
        "style": "", 
        "subtitle": "", 
        "title": "墨梅图题诗"
    }, 
    {
        "author": "范成大", 
        "content": "昼出耘田夜绩麻，\n村庄儿女各当家。\n童孙未解供耕织，\n也傍桑阴学种瓜。", 
        "explanation": "", 
        "note": "", 
        "prologue": "", 
        "style": "", 
        "subtitle": "其一", 
        "title": "四时田园杂兴"
    }, 
    {
        "author": "范成大", 
        "content": "梅子金黄杏子肥，\n麦花雪白菜花稀。\n日长篱落无人过，\n惟有蜻蜓蛱蝶飞。", 
        "explanation": "", 
        "note": "", 
        "prologue": "", 
        "style": "", 
        "subtitle": "其二", 
        "title": "四时田园杂兴"
    }, 
    {
        "author": "高适", 
        "content": "千里黄云白日曛，\n北风吹燕雪纷纷。\n莫愁前路无知己，\n天下谁人不识君。", 
        "explanation": "", 
        "note": "", 
        "prologue": "", 
        "style": "", 
        "subtitle": "", 
        "title": "别董大"
    }, 
    {
        "author": "王维", 
        "content": "渭城朝雨浥轻尘，\n客舍青青柳色新。\n劝君更尽一杯酒，\n西出阳关无故人。", 
        "explanation": "", 
        "note": "", 
        "prologue": "", 
        "style": "", 
        "subtitle": "", 
        "title": "送元二使安西"
    }
]